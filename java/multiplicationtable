package Problems;
import java.util.*;
public class multiplicationtable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number:");
		int a=sc.nextInt();
		for(int i=1;i<=10;i++) {
			System.out.printf("%d * %d = %d\n",a,i,a*i);
		}
	}

}
