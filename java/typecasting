package Problems;
import java.util.*;
public class typecasting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		double d=3.000;
		int x=3;
		int a=(int)d;
		double c= (double)x;
		System.out.println("Double to Int:"+a);
		System.out.println("Int to Double:"+c);
		
	}

}
